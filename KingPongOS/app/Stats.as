package app {	import flash.display.MovieClip;	import app.Profile;	public class Stats extends MovieClip {				public function stats(){		}				public function getStats(p:Profile){			this.cred.text = (""+p.getCredits());			this.kd.text = (p.getWins()+"/"+p.getLosses());			this.shotmiss.text = (p.getCH()+"/"+p.getCD());			this.rank.text = (""+p.updateRank());		}				public function clearStats(){			this.who.text = "";			this.cred.text = "";			this.kd.text = "";			this.shotmiss.text = "";			this.rank.text = "";		}	}}