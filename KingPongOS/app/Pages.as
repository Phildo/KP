package app {	import flash.display.MovieClip;	public class Pages extends MovieClip {		//Variables		var nextPage:String;				//Constructor		public function Pages(){			nextPage = "home";		}				//Meant to be called from KPOS. Takes integer		//input, and maps it to whatever page desired.		public function goTo(nP:int){			switch (nP)			{				case 0:				nextPage = "home";				break;				case 1:				nextPage = "list";				break;				case 2:				nextPage = "profile";				break;				case 3:				nextPage = "highscore";				break;				case 4:				nextPage = "stats";				break;				default:				nextPage = "home";			}			this.play();		}				//Called from inside movie clip. After one page		//successfully fades out, this fades next page in.		public function startNext(){			this.gotoAndPlay(nextPage);		}			}}